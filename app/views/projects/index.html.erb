		<div class="container" id="portfolio" name="portfolio">
		<br>
			<div class="row">
				<br>
				<h1 class="centered">PROJECTS</h1>
        <hr>
				<br>
				<br>
			</div><!-- /row -->
			<div class="container">
			<div class="row">	
			
				<!-- PORTFOLIO IMAGE 1 -->
        <% @projects.each_with_index do |project, index| %>
          <% if index == @projects.length - 1 && index % 2 == 0 %>
            <% offset = "col-md-offset-3" %>
          <% end %>
          <div class="col-md-6 <%= offset %>">
			    	<div class="grid mask">
						<figure>
            <img class="img-responsive" src="<%= image_url(project.image_path) %>" alt="">
							<figcaption>
              <h5><%= project.name %></h5>
              <a data-toggle="modal" href="#<%= project.short_name %>" class="btn btn-primary btn-lg">Take a Look</a>
							</figcaption><!-- /figcaption -->
						</figure><!-- /figure -->
			    	</div><!-- /grid-mask -->
				</div><!-- /col -->
				
				
						 <!-- MODAL SHOW THE PORTFOLIO IMAGE. In this demo, all links point to this modal. You should create
						      a modal for each of your projects. -->
						      
                  <div class="modal fade" id="<%= project.short_name %>" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
						    <div class="modal-dialog">
						      <div class="modal-content">
						        <div class="modal-header">
						          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                      <h4 class="modal-title"><%= project.name %></h4>
						        </div>
						        <div class="modal-body">
						          <p><img class="img-responsive" src="<%= image_url(project.image_path) %>" alt=""></p>
                      <p><%= project.description.html_safe %></p>
                      <p>Technologies: <%= project.technologies %></p>
                      <p>
                      <% if project.live_url %>
                        <b><a href="<%= project.live_url %>" target="_blank">View live</a></b> |
                      <% end %>
                      <b><a href="<%= project.github_url %>" target="_blank">View on Github</a></b>
                      </p>
						        </div>
						        <div class="modal-footer">
						          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
						        </div>
						      </div><!-- /.modal-content -->
						    </div><!-- /.modal-dialog -->
						  </div><!-- /.modal -->
				
    <% end %>
      </div><!-- row -->
